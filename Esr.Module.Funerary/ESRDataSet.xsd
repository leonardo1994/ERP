                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        OS.NumberOs, OS.DateRegistration, SUM(OsProduct.ValueTotal) + SUM(OsService.ValueService) AS ValorTotalOS
